{
    "$schema": "http://json.schemastore.org/template",
    "author": "Melad",
    "classifications": ["Api"],
    "name": "Melad.CleanArchitecture.Template",
    "defaultName": "Melad.CleanArchitecture.Template",
    "identity": "Melad.CleanArchitecture.Template",
    "shortName": "Melad.CA",
    "tags": {
      "language": "C#",
      "type": "project"
    },
    "sourceName": "Melad.CleanArchitecture.Template",
    "preferNameDirectory": true,
    "description": "Written By Melad",
  "symbols": {
    "SubcommandName": {
      "type": "parameter",
      "description": "RootName",
      "replaces": "Melad.CleanArchitecture",
      "defaultValue": "Melad.CleanArchitecture"
    },
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net8.0"
        }
      ],
      "defaultValue": "net8.0",
      "replaces": "{TargetFramework}"
    },
    "EnableSwaggerSupport": {
      "type": "parameter",
      "description": "Enable swagger support",
      "datatype": "bool",
      "defaultValue": "true"
    }
  }
}